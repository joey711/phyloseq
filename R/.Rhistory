#' @aliases mt,otuTable,logical-method#
#' @rdname mt-methods#
setMethod("mt", c("otuTable", "logical"), function(physeq, classlabel, minPmaxT="minP", ...){#
	mt(physeq, as(classlabel, "integer"), minPmaxT="minP", ...)#
})
